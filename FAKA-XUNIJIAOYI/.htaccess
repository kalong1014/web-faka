# Helicon ISAPI_Rewrite configuration file
# Version 3.1.0.73

RewriteEngine on
RewriteBase /

RewriteCond %{HTTP_HOST} ^hzskdz.com [NC] 
RewriteRule ^(.*)$ http://www.hzskdz.com/$1 [L,R=301]

RewriteRule ^(.*)/search(.*).html /$1/index.php\?str=$2
RewriteRule ^(.*)/view([0-9]*).html /$1/view.php\?id=$2
RewriteRule ^(.*)/(.*)list(.*).html /$1/$2list.php\?str=$3
RewriteRule ^(.*)/(.*)view(.*).html /$1/$2view.php\?id=$3

RewriteRule upload/(.*).(PHP)$ 每 [L,NC] 
RewriteRule upload/(.*).(asp)$ 每 [L,NC] 
RewriteRule upload1/(.*).(php)$ 每 [L,NC] 
RewriteRule upload1/(.*).(asp)$ 每 [L,NC] 
RewriteRule upload2/(.*).(php)$ 每 [L,NC] 
RewriteRule upload2/(.*).(asp)$ 每 [L,NC] 
RewriteRule upload3/(.*).(php)$ 每 [L,NC] 
RewriteRule upload3/(.*).(asp)$ 每 [L,NC] 
RewriteRule ad/(.*).(PHP)$ 每 [L,NC] 
RewriteRule ad/(.*).(asp)$ 每 [L,NC] 
RewriteRule uploadfile/(.*).(php)$ 每 [L,NC] 
RewriteRule uploadfile/(.*).(asp)$ 每 [L,NC] 
RewriteRule userphoto/(.*).(php)$ 每 [L,NC] 
RewriteRule userphoto/(.*).(asp)$ 每 [L,NC] 
RewriteRule attached/(.*).(php)$ 每 [L,NC] 
RewriteRule attached/(.*).(asp)$ 每 [L,NC]
RewriteRule img/(.*).(php)$ 每 [L,NC] 
RewriteRule img/(.*).(asp)$ 每 [L,NC]